{
  "name": "@ritem/monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@ritem/core": "workspace:*",
    "@ritem/footer":"workspace:*",
    "@elonehoo/eslint-config": "^2.0.0",
    "@iconify-json/carbon": "^1.1.4",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@unocss/reset": "^0.32.8",
    "@vitejs/plugin-react": "^1.3.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "pnpm": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "unocss": "^0.32.8",
    "vite": "^2.9.5",
    "vitest": "^0.10.0"
  }
}
