{
  "name": "@ritem/monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.24.2",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "example:dev":"npm -C example run dev",
    "example:build":"npm -C example run build",
    "example:preview":"npm -C example run preview"
  },
  "devDependencies": {
    "@ritem/core": "workspace:*",
    "@ritem/footer":"workspace:*",
    "@ritem/hi":"workspace:*",
    "@ritem/all":"workspace:*",
    "@elonehoo/eslint-config": "2.0.0",
    "@iconify-json/carbon": "1.1.13",
    "@types/node": "17.0.45",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@unocss/reset": "0.48.3",
    "@vitejs/plugin-react": "2.2.0",
    "bumpp": "8.2.1",
    "eslint": "8.31.0",
    "pnpm": "7.24.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.6.2",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "unbuild": "0.9.4",
    "unocss": "0.48.3",
    "vite": "4.2.1",
    "vitest": "0.27.0"
  }
}
